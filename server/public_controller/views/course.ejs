        </div>
        <div class='card-body'>
          <ul class="list-group list-group-flush">
            <queue-item v-for="(ticket, index) in getTickets()" 
              :student="ticket"
              :key="index"
              >
            </queue-item>
          </ul>
        </div>
      </div>
